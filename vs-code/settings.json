{
    "editor.tabSize": 2,
    "editor.renderWhitespace": "none",
    "editor.fontSize": 14,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "diffEditor.renderSideBySide": false,
    "explorer.confirmDragAndDrop": false,
    "[elixir]": {
        "editor.defaultFormatter": "JakeBecker.elixir-ls"
    },
    "files.associations": {
        "*.js.erb": "javascript",
        "*.md.erb": "markdown"
    },
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "diffEditor.ignoreTrimWhitespace": false,
    "extensions.ignoreRecommendations": false,
    "elixirLS.dialyzerFormat": "dialyxir_long",
    "workbench.activityBar.visible": false,
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "none",
    "editor.cursorWidth": 3,
    "editor.find.seedSearchStringFromSelection": false,
    "workbench.quickOpen.closeOnFocusLost": false,
    "workbench.quickOpen.preserveInput": true,
    "explorer.decorations.colors": false,
    "search.showLineNumbers": true,
    "workbench.colorTheme": "Monokai Dimmed",
    "workbench.colorCustomizations": {
        "[Monokai Dimmed]": {
            "foreground": "#a7a7a7",
            "activityBar.foreground": "#a7a7a7",
            "editor.foreground": "#a7a7a7",
            "tab.inactiveBackground": "#282828",
            "tab.inactiveForeground": "#b8b8b8",
            "tab.activeForeground": "#b8b8b8",
            "badge.background": "#333",
            "badge.foreground": "#a7a7a7",
            "editorLineNumber.foreground": "#505050",
            "editorIndentGuide.background": "#333",
            "statusBar.background": "#242424",
            "statusBar.foreground": "#727272",
            "panelTitle.activeForeground": "#a7a7a7"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Monokai Dimmed]": {
            "comments": "#666",
            "textMateRules": [
                {
                    "name": "Class name",
                    "scope": "entity.name.class, entity.name.type, entity.name.namespace, entity.name.scope-resolution",
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#9B3333",
                    }
                }
            ]
        }
    },
    "editor.wordWrapColumn": 100,
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "beforeWhitespace",
    "editor.autoClosingQuotes": "beforeWhitespace",
    "editor.folding": true,
    "editor.hover.delay": 250,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "elixirLS.fetchDeps": false,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.tabCloseButton": "left",
    "search.quickOpen.history.filterSortOrder": "recency",
    "gitlens.advanced.telemetry.enabled": false,
    "ruby.intellisense": "rubyLocate",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.startupEditor": "newUntitledFile",
    "elixirLS.suggestSpecs": false,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "gitlens.blame.highlight.locations": [
        "gutter",
        "line",
        "overview"
    ],
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "gitlens.menus": {
        "editorGroup": {
            "blame": true,
            "compare": true
        },
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scm": {
            "authors": true
        },
        "scmGroupInline": {
            "stash": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": false,
            "stash": true
        }
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.formatOnSave": true,
    "window.zoomLevel": 1
}
